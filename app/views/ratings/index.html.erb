<p id="notice"><%= notice %></p>

<h1>Listing Ratings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Place</th>
      <th>Ease rating</th>
      <th>Safety rating</th>
      <th>Enjoyability rating</th>
      <th>Accessibility rating</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ratings.each do |rating| %>
      <tr>
        <td><%= rating.user_id %></td>
        <td><%= rating.place_id %></td>
        <td><%= rating.ease_rating %></td>
        <td><%= rating.safety_rating %></td>
        <td><%= rating.enjoyability_rating %></td>
        <td><%= rating.accessibility_rating %></td>
        <td><%= rating.comment %></td>
        <td><%= link_to 'Show', rating %></td>
        <td><%= link_to 'Edit', edit_rating_path(rating) %></td>
        <td><%= link_to 'Destroy', rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rating', new_rating_path %>
